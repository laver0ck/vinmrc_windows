set guifont=Consolas:h11
language messages en_US.UTF-8
:set encoding=utf8
:set termencoding=utf8
:set fileencoding=utf8
syntax on
colorscheme desert
filetype plugin indent on

inoremap kj <ESC>
set backspace=indent,eol,start
set autochdir

set shiftwidth=2
set tabstop=2
set softtabstop=2
set expandtab
set shiftround
set autoindent

